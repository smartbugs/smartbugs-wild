            uint256 wdamount = balances[msg.sender][0];
            balances[msg.sender][0] = 0;
            ForeignToken tokenbase = ForeignToken(token);
            address dest = msg.sender;
            return tokenbase.transfer(dest, wdamount);
        }else{
